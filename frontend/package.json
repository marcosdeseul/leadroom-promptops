{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5300",
    "build": "next build",
    "start": "next start -p 5300",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:storybook": "test-storybook --url http://127.0.0.1:5400",
    "test:storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --port 5400 --silent\" \"wait-on tcp:127.0.0.1:5400 && npm run test:storybook\"",
    "storybook": "storybook dev -p 5400",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.4.2",
    "@chakra-ui/react": "^3.27.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.2",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.64.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.2.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-onboarding": "9.1.10",
    "@storybook/nextjs": "9.1.10",
    "@storybook/test-runner": "^0.23.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-storybook": "9.1.10",
    "http-server": "^14.1.1",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "storybook": "9.1.10",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.1"
  }
}
